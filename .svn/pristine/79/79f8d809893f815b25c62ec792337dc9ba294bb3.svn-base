<div class="game-history container">
    <div class="modal-header">
        <h1 class="title">
            <span>{{data.title}}</span> Histroy
        </h1>

        <button type="button" name="button" (click)="close()" class="close-btn">
            <img src="/assets/images/common/close-btn.png" alt="">
        </button>
    </div>
<ng-template [ngIf]="data.title === 'fifty'">
    <mat-tab-group mat-align-tabs="center" animationDuration="0ms" [selectedIndex]="data.type" (selectedIndexChange)="selectedIndexChanged($event)">
        <mat-tab label="{{ 'games.list.recent' | translate}}">
            <div class="history-box">
                <div class="table-home">
                        <table class="table table-striped table-borderless">
                            <colgroup>
                                <col width="10%">
                                <col width="10%" class="mobile-none">
                                <col width="*">
                            </colgroup>
                            <thead>
                                <tr>
                                    <th scope="col">#</th>
                                    <th scope="col" class="mobile-none">{{ 'games.list.title_result' | translate}}</th>
                                    <th scope="col">Game</th>
                                </tr>
                            </thead>
                            <tbody>
                                <ng-container *ngFor="let data of recentGamesHistory; let i = index;">
                                    <tr>
                                        <td>{{ data.id }}</td>
                                        <td class="mobile-none">
                                            {{ 'games.fifty.result.' + data.result_code  | translate }}
                                            ({{ data.result_number }})
                                        </td>
                                        <td>
                                            <ul>
                                                <li class="mobile-flex">
                                                    <span class="point">{{ 'games.list.title_result' | translate}} :</span>
                                                    <p>{{ 'games.fifty.result.' + data.result_code  | translate }} ({{ data.result_number }})</p>
                                                </li>
                                                <li>
                                                    <span class="point">{{ 'games.list.title_hash' | translate}} :</span>
                                                    <p (click)="copyToClipboard(data.hash)">{{ data.hash }}</p>
                                                </li>
                                                <li>
                                                    <span class="point">{{ 'games.list.title_salt' | translate}} :</span>
                                                    <p (click)="copyToClipboard(data.salt)">{{data.salt}}</p>
                                                </li>
                                                <li>
                                                    <span class="point">{{ 'games.list.title_date' | translate}} :</span>
                                                    <p class="create">{{data.datetime | momentLocal : 'YYYY-MM-DD HH:mm:ss'}}</p>
                                                </li>
                                            </ul>
                                        </td>
                                    </tr>
                                </ng-container>

                                <tr class="no-display" *ngIf="recentGamesHistory?.length === 0">
                                    <td colspan="5" class="mobile-none">
                                        {{ 'games.list.no_list' | translate}}
                                    </td>
                                    <td class="mobile-cell">
                                        {{ 'games.list.no_list' | translate}}
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                        <button (click)="more()" class="more" *ngIf="this.moreBtn.gameHis">{{ 'button.more' | translate}}</button>
                </div>
            </div>
        </mat-tab>
        <mat-tab label="{{ 'games.list.my' | translate}}">
            <div class="history-box">
                <div class="table-home">
                        <table class="table table-striped table-borderless">
                            <colgroup>
                                <col width="10%">
                                <col width="10%" class="mobile-none">
                                <col width="*" class="mobile-cell">
                                <col width="*" class="mobile-none">
                                <col width="*" class="mobile-none">
                                <col width="*" class="mobile-none">
                                <col width="*" class="mobile-none">
                            </colgroup>
                            <thead>
                                <tr>
                                    <th scope="col">#</th>
                                    <th scope="col" class="mobile-none">{{ 'games.list.title_choice' | translate}}</th>
                                    <th scope="col" class="mobile-cell">Game</th>
                                    <th scope="col" class="mobile-none">{{ 'games.list.title_bet_amount' | translate}}</th>
                                    <th scope="col" class="mobile-none">{{ 'games.list.title_result' | translate}}</th>
                                    <th scope="col" class="mobile-none">{{ 'games.list.title_win_amount' | translate}}</th>
                                    <th scope="col" class="mobile-none">{{ 'games.list.title_date' | translate}}</th>
                                </tr>
                            </thead>
                            <tbody>
                                <ng-container *ngFor="let data of myGamesHistory; let i = index;">
                                    <tr>
                                        <td>{{ data.game_id }}</td>
                                        <td class="mobile-none">{{ 'games.fifty.result.' + data.choice  | translate }}</td>
                                        <td class="mobile-cell">
                                            <ul class="my-game">
                                                <li class="mobile-flex">
                                                    <span class="point">{{ 'games.list.title_choice' | translate}} :</span>
                                                    <p>{{ 'games.fifty.result.' + data.choice  | translate }}</p>
                                                </li>
                                                <li>
                                                    <span class="point">{{ 'games.list.title_bet_amount' | translate}} :</span>
                                                    <p>{{data.bet_amount | number}}</p>
                                                </li>
                                                <li>
                                                    <span class="point">{{ 'games.list.title_win_amount' | translate}} :</span>
                                                    <p>{{data.win_amount | number}}</p>
                                                </li>
                                                <li>
                                                    <span class="point">{{ 'games.list.title_result' | translate}} :</span>
                                                    <p>{{'games.result.' + data.result  | translate}}</p>
                                                </li>
                                                <li>
                                                    <span class="point">{{ 'games.list.title_date' | translate}} :</span>
                                                    <p class="create">{{data.created_at | momentLocal : 'YYYY-MM-DD HH:mm:ss'}}</p>
                                                </li>
                                            </ul>
                                        </td>
                                        <td class="mobile-none">{{data.bet_amount | number}}</td>
                                        <td class="mobile-none">{{'games.result.' + data.result  | translate}}</td>
                                        <td class="mobile-none">{{data.win_amount | number}}</td>
                                        <td class="mobile-none">{{data.created_at | momentLocal : 'YYYY-MM-DD HH:mm:ss' }}</td>
                                    </tr>
                                </ng-container>
                                <tr class="no-display" *ngIf="myGamesHistory?.length === 0">
                                    <td colspan="6" class="mobile-none">
                                        {{ 'games.list.no_list' | translate}}
                                    </td>
                                    <td colspan="2" class="mobile-cell">
                                        {{ 'games.list.no_list' | translate}}
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                        <button (click)="more()" class="more" *ngIf="this.moreBtn.myHis">{{ 'button.more' | translate}}</button>
                </div>
            </div>
        </mat-tab>
    </mat-tab-group>
</ng-template>
