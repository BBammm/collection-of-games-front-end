::ng-deep{
    .mat-tab-header{
        border-color:#34424f;
    }
    .mat-tab-group.mat-primary .mat-ink-bar {
        background-color:#14dbcf;
    }
    mat-tab-group{
        .mat-tab-header-pagination-controls-enabled .mat-tab-header-pagination{
            display:none;
        }
        .mat-tab-header-pagination{
            display:none;
        }
        .mat-tab-label{
            opacity: 1;
            color:#8fa8c0;
            &.mat-tab-label-active{
                color: #fff;
            }
        }
    }

    .mat-slide-toggle-thumb{
        width: 12px !important;
        height: 12px !important;
        position: inherit;
        // top: 50%;
        // transform:translateY(-50%);
        background-color:#697b8c !important;
        top: 30%;
        left: 3px;
    }

    .mat-slide-toggle-bar{
        border:1px solid #697b8c;
        height: 20px !important;
        border-radius: 10px !important;
        order: 2;
    }

    .mat-slide-toggle.mat-checked .mat-slide-toggle-bar{
        background-color:transparent;
        border:1px solid #14dbcf;
    }

    .mat-slide-toggle.mat-checked .mat-slide-toggle-thumb{
        background-color: #14dbcf !important;
    }

    .mat-slide-toggle-content{
        margin-right: 5px;
        font-size: 14px;
        color:#95a6b5;
        font-family: 'Open Sans', 'Noto Sans KR', sans-serif;
    }

    .mat-slide-toggle.mat-checked .mat-slide-toggle-content{
        color: #14dbcf;
    }

    div.game-container.keno{
        max-width: 1230px;
        padding: 60px 15px;
        div.game-wrap{
            display: flex;
            div.game-left{
                padding: 30px;
                background-color: #15212c;
                width: 35%;
                order: 1;
                div.user-balance{
                    display:flex;
                    align-items:center;
                    span.title{
                        display: inline-block;
                        color:#95a6b5;
                        font-size: 13px;
                        img{
                            margin-right: 5px;
                        }
                    }
                    p.point{
                        margin-bottom: 0;
                        margin-left: 10px;
                        font-size: 13px;
                        color:#fff;
                        font-weight: bold;
                    }
                }
                div.bet-calculator{
                    margin: 30px 0 20px 0;
                    p.title{
                        font-size: 14px;
                        color:#95a6b5;
                        margin-bottom: 10px;
                    }
                }
                div.auto-check{
                }
                div.betting-btn{
                    margin-top: 10px;
                    margin-bottom: 30px;
                    display:flex;
                    justify-content: space-between;
                    button{
                        border-radius:5px;
                        color:#fff;
                        border-style: none;
                        height: 60px;
                        font-weight: bold;
                        transition: all 0.3s ease;
                        &.bet-btn{
                            width: 83%;
                            background: #14dbcf;
                            // display:none;
                            &:hover{
                                background-color:#0fcec2;
                            }
                        }
                        &.quick-btn{
                            width: 41%;
                            background:#53e996;
                        }
                        &.stop-btn{
                            width: 83%;
                            background:#517797;
                        }
                        &.reset-btn{
                            width: 15%;
                            background-color: #2d516f;
                        }
                        &.restart{
                            background:#53e996;
                        }
                    }
                }
                div.code-wrap{
                    margin-top: 25px;
                    div.code-box{
                        p.title{
                            font-size: 14px;
                            color:#95a6b5;
                            margin-bottom: 5px;
                        }
                        div.field{
                            display:flex;
                            mat-form-field{
                                width: 85%;
                                .mat-form-field-wrapper{
                                    .mat-form-field-outline{
                                        border-radius: 5px 0 0 5px;
                                    }
                                }
                            }
                            button{
                                width: 15%;
                                height: 35px;
                                border-style: none;
                                background: #223342;
                                border-radius:0 5px 5px 0;
                            }
                        }
                    }
                }
            }
            div.game-right{
                padding: 30px;
                background-color: #1a2732;
                width: 65%;
                display: flex;
                flex-wrap: wrap;
                div.game-round-text{
                    position: relative;
                    width: 100%;
                    p.title{
                        font-size: 22px;
                        font-weight: bold;
                        margin-bottom: 15px;
                        color:#91a2b2;
                        span{
                            color:#fff;
                        }
                    }
                }
                div.pay-out{
                    // border:1px solid gold;
                    display:flex;
                    flex:1 1 auto;
                    // padding: 25px 10px;
                    flex-direction: column;
                    max-width: 175px;
                    div.img-box{
                        position: relative;
                        z-index: 1;
                        img{
                            width: 100%;
                        }
                    }
                    ul{
                        // border:1px solid #fff;
                        list-style: none;
                        padding: 0;
                        width: 100%;
                        height: 100%;
                        margin-bottom: 0;
                        background-color:#15212c;
                        margin-top: -10px;
                        padding: 15px;
                        li{
                            display: flex;
                            justify-content: space-between;
                            word-break: break-all;
                            line-height: 25px;
                            transition: all 0.5s ease;
                            span{
                                font-size: 12px;
                                &.list-number{
                                    color: #5e6d79;
                                }
                                &.list-data{
                                    color: #95a6b5;
                                }
                            }
                        }
                    }

                }
                div.tile-wrap{
                    display:flex;
                    flex-wrap: wrap;
                    max-width: 545px;
                    justify-content: space-between;
                    padding-left: 10px;
                    div.tile{
                        // border:1px solid #fff;
                        width: 9.1%;
                        p{
                            height: 50px;
                            line-height: 50px;
                            margin-bottom: 5px;
                            box-shadow: inset 0px -4px 0 0 rgba(0, 0, 0, 0.19);
                            background-color: #3b4d5b;
                            border-radius: 5px;
                            cursor: pointer;
                            text-align: center;
                            position: relative;
                            span{
                                font-size: 16px;
                                color:#7a8d9c;
                                font-family: "Russo One", "Noto Sans KR", sans-serif;
                            }
                            &.pressed {
                                background-color: #3c83ee ;
                                span{
                                    color:#fff;
                                }
                            }
                            &.bomb::after{
                                content:'';
                                width: 100%;
                                height: 100%;
                                position: absolute;
                                left: 0;
                                top: 0;
                                border:2px solid #ffba00;
                                border-radius: 5px;
                            }
                            &.bingo {
                                background-color: #f84f4f ;
                            }
                        }
                    }
                }
            }
        }
        div.history-wrap{
            background-color: #2b3c4c;
            margin-top:30px;
            div.verification-btn{
                text-align: right;
                background-color: #212e3a;
                margin-bottom: 10px;
                padding-bottom: 10px;
                button{
                    font-size: 13px;
                    border-style: none;
                    outline: none;
                    background: none;
                    color:#95a6b5;
                    padding: 6px 15px;
                    border-radius: 15px;
                    background-color: #273643;
                    i{
                        color:#626f7c;
                        font-size: 19px;
                        vertical-align: bottom;
                        margin-right: 5px;
                    }
                    &:hover{
                        color:#fff;
                        i{
                            color:#14dbcf;
                        }
                    }
                }
            }
            mat-tab-group{
                div.history-box{
                    padding: 30px 50px;
                    background-color: #2b3c4c;
                    h2.title{
                        color:#fff;
                        font-size: 18px;
                        text-align: center;
                        margin-bottom: 30px;
                        font-weight: bold;
                    }
                    div.table-home{
                        table{
                            table-layout: fixed;
                            .mobile-block{
                                display: none;
                            }
                            thead{
                                 background-color: #3e6c96;
                                 tr{
                                     th{
                                         color:#fff;
                                         font-size: 14px;
                                         text-align: center;
                                         font-weight: normal;
                                     }
                                 }
                            }
                            tbody{
                                tr{
                                    background-color: #3c5062;
                                    td{
                                        text-align: center;
                                        color:#94a5b5;
                                        font-size: 14px;
                                        p{
                                            text-overflow: ellipsis;
                                            margin-bottom: 0;
                                            overflow: hidden;
                                            display: inline-block;
                                            cursor: pointer;
                                            width: 80%;
                                            height: 20px;
                                            white-space: nowrap;
                                        }
                                    }
                                    &.no-display{
                                        td{
                                            text-align: center;
                                        }
                                    }
                                    &:nth-child(odd){
                                        background-color: #2b3c4c;
                                    }
                                }
                            }
                        }
                        nav{
                            ul{
                                text-align: center;
                                justify-content: center;
                                margin-bottom: 0;
                                li{
                                    a.page-link{
                                        color:#627281;
                                        background: none;
                                        border:none;
                                        font-size: 12px;
                                        font-weight: bold;
                                        i{
                                            font-size: 10px;
                                        }
                                    }
                                    &.active{
                                        a{
                                            color:#95a6b5;
                                        }
                                    }
                                }
                            }
                        }
                        div.more-btn{
                            text-align: center;
                            button{
                                border-style: none;
                                background: #2d516f;
                                border-radius: 5px;
                                color: #fff;
                                font-size: 13px;
                                padding: 5px 15px;
                            }
                        }
                    }
                }
            }
        }
    }


    // Extra large devices (large desktops, 1200px and up)
    @media (min-width: 1025px) and (max-width: 1225px){

        div.game-container.keno{
            div.game-wrap{
                div.game-left{
                    width:40%;
                }
                div.game-right{
                    width:60%;
                }
            }
        }
    }

    // mobile or tablets
    @media (max-width: 992px) {}

    @media (max-width: 1250px) {
        div.game-container.keno{
            div.game-wrap{
                div.game-right{
                    div.pay-out{
                        max-width: 100%;
                        flex-direction: inherit;
                        div.img-box{
                            img{
                                width: auto;
                            }
                        }
                        ul{
                            margin-top: 0;
                            margin-left: -10px;
                            li{
                                display: inline-block;
                                width: 50%;
                            }
                        }
                    }
                    div.tile-wrap{
                        margin: auto;
                        padding:0;
                    }
                }
            }
        }
    }


    @media (max-width: 1024px) {

        div.game-container.keno{
            div.game-wrap{
                div.game-left{
                    width:50%;
                }
                div.game-right{
                    width:50%;
                    div.pay-out{
                        padding: 25px 0;
                         div.img-box{
                             display: none;
                         }
                         ul{
                             margin-left: 0;
                         }
                    }
                    div.tile-wrap{
                        div.tile{
                            p{
                                height: 30px;
                                line-height: 30px;
                            }
                        }
                    }
                }
            }
        }

    }



    @media (max-width: 768px) {
        div.game-container.keno{
            padding: 15px;
            div.game-wrap{
                flex-wrap: wrap;
                div.game-left{
                    width:100%;
                    padding: 15px;
                    order:1;
                    div.game-history-wrap{
                        padding: 10px;
                    }
                }
                div.game-right{
                    width:100%;
                    padding: 15px;
                    div.pay-out{
                        padding: 25px 0;
                        order: 1;
                        width: 100%;
                    }
                    div.tile-wrap{
                       div.tile{
                           p{
                               height: 50px;
                               line-height: 50px;
                           }
                       }
                   }
                }
            }

            div.history-wrap{
                mat-tab-group{
                    div.history-box{
                        padding: 20px 15px;
                        div.table-home{
                            table{
                                .mobile-none{
                                    display:none;
                                }
                                .mobile-block{
                                    display: block;
                                    mat-accordion{
                                        mat-expansion-panel{
                                            border-radius: 0 !important;
                                            background: none;
                                            &:not([class*=mat-elevation-z]){
                                                box-shadow: none;
                                            }
                                            mat-expansion-panel-header{
                                                padding: 0;
                                                &:hover{
                                                    background: none;
                                                }
                                                mat-panel-title{
                                                    color:#94a5b5;
                                                    font-size: 14px;
                                                }
                                                span.mat-expansion-indicator{
                                                    padding-right: 5px;
                                                    &::after{
                                                        color:#94a5b5;
                                                    }
                                                }
                                            }
                                            div.mat-expansion-panel-body{
                                                padding: 0;
                                                div.game-info{
                                                    display: flex;
                                                    margin-bottom:5px;
                                                    color:#94a5b5;
                                                    font-size: 14px;
                                                    span{
                                                        // width: 20%;
                                                        color:#94a5b5;
                                                        text-align: left;
                                                        margin-right: 5px;
                                                    }
                                                    p{
                                                        color:#53e996;
                                                        text-align: left;
                                                    }
                                                    &.data{
                                                        text-align: right;
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }

    // Medium devices (tablets, 768px and up)
    @media (min-width: 577px) and (max-width: 992px) {


    }


    // Small devices (landscape phones, 576px and up)
    @media (max-width: 576px) {
        div.game-container.keno{
            div.game-wrap{
                div.game-left{
                }
                div.game-right{
                    div.pay-out{
                         div.img-box{
                             display: none;
                         }
                         ul{
                             margin-left: 0;
                         }
                    }
                    div.tile-wrap{
                       div.tile{
                           p{
                               height: 30px;
                               line-height: 30px;
                           }
                       }
                   }
                }
            }
        }
    }

}
