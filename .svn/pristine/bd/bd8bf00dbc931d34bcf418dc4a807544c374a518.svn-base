.chat-box{overflow-wrap:break-word;position:relative;filter:drop-shadow(rgba(25,25,25,0.25) 0px 0px 5px);display:flex;flex-direction:column;z-index:900;width:355px;flex-shrink:0;height:100vh;background:#222e3a}.chat-box .chat-box-head{width:100%;min-height:120px;height:120px;background:#222e3a;padding:25px 40px 0}.chat-box .chat-box-head .chat-close button{background:none;outline:none;border-style:none}.chat-box .chat-box-head .channel-select-btn{font-size:inherit;touch-action:manipulation;position:relative;display:inline-flex;-webkit-box-pack:center;justify-content:center;-webkit-box-align:center;align-items:center;color:#fff;background:none;border-style:none;margin-top:20px}.chat-box .chat-box-head .channel-select-btn .cur-lan img{padding-right:10px;display:inline-block;width:30px}.chat-box .chat-box-head .channel-select-btn .cur-lan span{display:inline-block}.chat-box .split-line{overflow-x:auto;overflow-y:hidden;transform:translate3d(0px, 0px, 0px);width:100%;padding:0px 1rem;flex:0 0 auto;border-bottom:2px solid #505050}.chat-box .split-line .split-line-inner{display:flex;align-items:flex-end}.chat-box .chat-body-channel{display:flex;flex-direction:column;-webkit-box-flex:1;flex-grow:1;width:100%;overflow:hidden;background:#000;position:relative}.chat-box .chat-body-channel>section{position:absolute;left:0px;right:0px;top:0px;bottom:0px;display:flex;flex-direction:column;overflow:hidden;background:#15212d}.chat-box .chat-body-channel>section>ul{overflow-y:auto;overflow-x:hidden;min-height:0px;width:100%;padding:0 40px}.chat-box .chat-body-channel>section>ul>li{display:flex;-webkit-box-align:center;align-items:center;-webkit-box-pack:justify;justify-content:space-between}.chat-box .chat-body-channel>section>ul>li>button{font-size:inherit;touch-action:manipulation;position:relative;display:inline-flex;-webkit-box-pack:center;justify-content:center;-webkit-box-align:center;align-items:center;color:#fff;background:none;border-style:none;margin-top:15px}.chat-box .chat-body-channel>section>ul>li>button>span{display:inline-flex;-webkit-box-align:center;align-items:center;position:relative}.chat-box .chat-body-channel>section>ul>li>button>span>*+*{margin-left:0.5em}.chat-box .chat-body-channel>section>ul>li>button>span>img{display:inline-block;width:20px}.chat-box .chat-body-channel .chat-lists{width:100%;-webkit-box-flex:1;flex-grow:1;display:flex;flex-direction:column;-webkit-box-pack:end;justify-content:flex-end;overflow:hidden}.chat-box .chat-body-channel .chat-lists .chat-list-layout{width:100%;-webkit-box-flex:1;flex-grow:1;display:flex;flex-direction:column;-webkit-box-pack:end;justify-content:flex-end;overflow:hidden;background:#15212d}.chat-box .chat-body-channel .chat-lists .chat-list-layout .chats{overflow-x:hidden;min-height:0px;transform:translate3d(0px, 0px, 0px);overflow-y:scroll;word-break:break-word;width:100%;padding:30px 40px;background:#15212d}.chat-box .chat-body-channel .chat-lists .chat-list-layout .chats::-webkit-scrollbar{background-color:transparent;width:8px}.chat-box .chat-body-channel .chat-lists .chat-list-layout .chats::-webkit-scrollbar-track{background-color:transparent}.chat-box .chat-body-channel .chat-lists .chat-list-layout .chats::-webkit-scrollbar-track:hover{background-color:#0c16208c}.chat-box .chat-body-channel .chat-lists .chat-list-layout .chats::-webkit-scrollbar-thumb{background-color:#354758;border-radius:8px}.chat-box .chat-body-channel .chat-lists .chat-list-layout .chat-list{width:100%;margin:0.5em 0}.chat-box .chat-body-channel .chat-lists .chat-list-layout .chat-list>span.name{white-space:nowrap}.chat-box .chat-body-channel .chat-lists .chat-list-layout .chat-list>span.name>a{font-size:12px;touch-action:manipulation;position:relative;display:inline-flex;-webkit-box-pack:center;justify-content:center;-webkit-box-align:center;align-items:center;color:#e9ebe4}.chat-box .chat-body-channel .chat-lists .chat-list-layout .chat-list .item{font-weight:inherit;font-size:13px;color:#9ea9b5;margin-bottom:0;padding:15px;border-radius:5px;background-color:#313c48;position:relative}.chat-box .chat-body-channel .chat-lists .chat-list-layout .chat-list .item:before{content:'';position:absolute;width:8px;height:8px;left:-8px;background:url("/assets/images/common/Triangle-defualt.png")}.chat-box .chat-body-channel .chat-lists .chat-list-layout .chat-list.me{text-align:right}.chat-box .chat-body-channel .chat-lists .chat-list-layout .chat-list.me .item{background-color:#274f75;color:#8ca4ba;text-align:left}.chat-box .chat-body-channel .chat-lists .chat-list-layout .chat-list.me .item:before{content:'';position:absolute;width:8px;height:8px;left:inherit;right:-8px;transform:rotate(-90deg);background:url("/assets/images/common/Triangle-me.png")}.chat-box .chat-footer{width:100%;color:#fff;flex-shrink:0;position:relative;padding:20px;background:#182531}.chat-box .chat-footer div.send-box{display:flex;width:100%;justify-content:space-between}.chat-box .chat-footer div.send-box .chat-input{resize:none;background:#000;border-radius:5px;outline:0px;padding:0.5em;height:40px;width:85%;background:#0f1923;color:#6a7682;font-size:13px;border:none;padding:12px;overflow-y:hidden}.chat-box .chat-footer div.send-box .chat-input::placeholder{color:#6a7682}.chat-box .chat-footer div.send-box .chat-send-btn{width:40px;height:40px;border-radius:5px;background:#223241;border-style:none}.chat-box .chat-footer .chat-status{width:100%;display:flex;-webkit-box-align:center;align-items:center;-webkit-box-pack:justify;justify-content:space-between}.chat-box .chat-footer .chat-status .display-online{display:inline-flex;-webkit-box-align:center;align-items:center;min-width:10ch}.chat-box .chat-footer .chat-status .display-online-icon{display:inline-block;width:0.5em;height:0.5em;border-radius:0.25em;background:#00e449}.chat-box .chat-footer .chat-status .display-online-text{font-weight:600;font-size:0.8em;color:#b7bfd6}.chat-box .chat-footer .chat-status .input-controller{display:inline-flex;-webkit-box-align:center;align-items:center}.chat-box .chat-footer .chat-status .input-controller>*+*{margin-left:0.5em}.chat-box .chat-footer .chat-status .cnt-online{margin-bottom:0;margin-left:10px}.chat-box .chat-footer .chat-status .remain-text-count{font-weight:600;font-size:0.8em;color:#b7bfd6;margin-bottom:0}.chat-box .chat-footer .chat-status .chat-send-btn:disabled,.chat-box .chat-footer .chat-status .chat-send-btn[disabled]{opacity:0.5;cursor:not-allowed;pointer-events:none}.chat-box .chat-footer .chat-status .chat-send-btn{font-weight:600;font-size:0.8em;touch-action:manipulation;position:relative;display:inline-flex;-webkit-box-pack:center;justify-content:center;-webkit-box-align:center;align-items:center;line-height:1.5;color:#213843;transition:background 300ms ease 0s, opacity 300ms ease 0s, transform 100ms ease 0s;border-radius:5px;background:#00e403;padding:calc(0.5em + 2px) 1em}.chat-menu-footer{width:100%;height:40px;background-color:#000;padding:0 !important;color:#fff;z-index:100;font-weight:normal;font-size:10pt}.chat-menu-footer>hr{background-color:gray;height:1px;margin-top:5px;margin-bottom:5px}.chat-menu-footer>ul{margin:0 0 0 10px;padding:0;list-style:none;display:-webkit-box;display:-moz-box;display:-ms-flexbox;display:-webkit-flex;display:flex;-webkit-flex-flow:row nowrap;flex-flow:row nowrap;justify-content:flex-start}.chat-menu-footer>ul>li{cursor:pointer;margin:0 10px}
