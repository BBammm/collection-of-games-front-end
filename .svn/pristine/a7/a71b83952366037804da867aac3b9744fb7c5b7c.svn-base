::ng-deep{

    .mat-tab-header{
        border-color:#34424f;
    }
    .mat-tab-group.mat-primary .mat-ink-bar {
        background-color:#14dbcf;
    }
    mat-tab-group{
        .mat-tab-header-pagination-controls-enabled .mat-tab-header-pagination{
            display:none;
        }
        .mat-tab-header-pagination{
            display:none;
        }
        .mat-tab-label{
            opacity: 1;
            color:#8fa8c0;
            &.mat-tab-label-active{
                color: #fff;
            }
        }
    }

    div.game-container.graph{
        max-width: 1230px;
        padding: 60px 15px;
        // border:1px solid green;
        div.game-wrap{
            display: flex;
            div.game-left{
                padding: 30px;
                background-color: #15212c;
                width: 35%;
                order: 1;
                div.user-balance{
                    display:flex;
                    align-items:center;
                    span.title{
                        display: inline-block;
                        color:#95a6b5;
                        font-size: 13px;
                        img{
                            margin-right: 5px;
                        }
                    }
                    p.point{
                        margin-bottom: 0;
                        margin-left: 10px;
                        font-size: 13px;
                        color:#fff;
                        font-weight: bold;
                    }
                }
                div.bet-calculator{
                    margin-top: 20px;
                    p.title{
                        font-size: 14px;
                        color:#95a6b5;
                        margin-bottom: 10px;
                        display: flex;
                        align-items: flex-end;
                        input.stop-check{
                            width: 20px;
                            height: 20px;
                            border-radius: 5px;
                            margin-left: 5px;
                            cursor: pointer;
                        }
                    }
                    div.seeting{
                        // border:1px solid #fff;
                        display: flex;
                        align-items: center;
                        div.form-box{
                            position: relative;
                            margin-right: 2px;
                            input.cash-value{
                                background: #394a5a;
                                border-radius:5px;
                                width: 90px;
                                border-style: none;
                                color:#fff;
                                font-weight: bold;
                                height: 36px;
                                font-size: 14px;
                                padding:0 15px;
                                outline: none;
                            }
                            label{
                                position: absolute;
                                right: 8px;
                                line-height: 36px;
                                font-size: 14px;
                                color:#fff;
                                font-weight: bold;
                            }
                        }
                        div.up-down{
                            // border:1px solid pink;
                            display: flex;
                            button{
                                width: 36px;
                                height: 36px;
                                background-color:#223342;
                                margin: 0 2px;
                                border-style: none;
                                outline:none;
                                border-radius:5px;
                                display:flex;
                                align-items: center;
                                justify-content: center;
                                i{
                                    color:#94a5b5;
                                    font-size: 10px;
                                }
                            }
                        }
                    }
                }
                div.betting-btn{
                    margin-top: 20px;
                    margin-bottom: 30px;
                    button{
                        background: #14dbcf;
                        border-radius:5px;
                        color:#fff;
                        width: 100%;
                        border-style: none;
                        height: 60px;
                        font-weight: bold;
                        transition: all 0.3s ease;
                        &:hover{
                            background-color:#0fcec2;
                        }
                        &.restart{
                            background:#53e996;
                        }
                    }
                    div.cancel{
                        font-size: 13px;
                        color:#95a6b5;
                        margin-top: 5px;
                        a{
                            display:inline-block;
                            margin-left: 5px;
                            color:#14dbcf;
                            text-decoration: none;
                            cursor:pointer;
                        }
                    }
                }
            }
            div.game-right{
                padding: 30px;
                background-color: #1a2732;
                width: 65%;
                div.game-round-text{
                    position: relative;
                    p.title{
                        font-size: 22px;
                        font-weight: bold;
                        margin-bottom: 15px;
                        color:#91a2b2;
                        span{
                            color:#fff;
                        }
                    }
                }
                div.play-pannel {
                    position: relative;
                    display: flex;
                    height: 250px;
                    margin-top: 10px;
                    div.canvas-wrap{
                        width: 100%;
                        height: 100%;
                        margin: auto;
                        canvas {
                            margin: auto auto;
                            font-family: 'Russo One', 'Noto Sans KR', sans-serif;
                        }
                    }
                }
            }
        }
        div.bonus-game-info{
            background-color: #101d28;
            padding: 20px 30px;
            p.title{
                font-size: 14px;
                color:#95a6b5;
                margin-bottom: 5px;
            }
            div.game-bets-container{
                overflow-y: hidden;
                // min-height: 200px;
                margin-top: 15px;
                div.columns-list{
                    ul.column{
                        padding: 0;
                        margin-bottom: 0;
                        list-style: none;
                        div.game-bets-header{
                            background-repeat: no-repeat;
                            background-position: center;
                            display: flex;
                            justify-content:space-between;
                            color:#fff;;
                            font-size: 14px;
                            background: #275072;
                            li{
                                width: 25%;
                                text-align: center;
                                padding: 10px 0;
                            }
                        }
                        div.game-bets-list {
                            // -webkit-transform: scale3d(1, 1, 1);
                            // transform: scale3d(1, 1, 1);
                            // -webkit-animation-name: showed;
                            // -webkit-animation-duration: 0.2s;
                            // animation-name: showed;
                            // animation-duration: 0.2s;
                            font-size: 12px;
                            display: flex;
                            justify-content:space-between;
                            color:#94a5b5;
                            background-color: #182937;
                            &:nth-child(even){
                                background-color:#101d28;
                            }
                            li{
                                width: 25%;
                                text-align: center;
                                padding: 10px;
                            }
                            // @-webkit-keyframes showed {
                            //     0%   {-webkit-transform: scale3d(1, 0, 1); transform: scale3d(1, 0, 1);}
                            //     100% {-webkit-transform: scale3d(1, 1, 1);transform: scale3d(1, 1, 1);}
                            // }

                            // /* Standard syntax */
                            // @keyframes showed {
                            //     0%   {-webkit-transform: scale3d(1, 0, 1); transform: scale3d(1, 0, 1);}
                            //     100% {-webkit-transform: scale3d(1, 1, 1);transform: scale3d(1, 1, 1);}
                            // }
                            &.user-won li, &.user-won li a {color: #14dbcf;}
                            &.user-lost li, &.user-lost li a {color: #ff5252;}
                            &.user-playing li, &.user-playing li a {color: #94a5b5;}
                            &.user-cashed li, &.user-cashed li a {color: #14dbcf;}
                        }
                    }
                }
            }
        }
        div.history-wrap{
            background-color: #2b3c4c;
            margin-top:30px;
            position: relative;
            div.verification-btn{
                text-align: right;
                background-color: #212e3a;
                margin-bottom: 10px;
                padding-bottom: 10px;
                button{
                    font-size: 13px;
                    border-style: none;
                    outline: none;
                    background: none;
                    color:#95a6b5;
                    padding: 6px 15px;
                    border-radius: 15px;
                    background-color: #273643;
                    i{
                        color:#626f7c;
                        font-size: 19px;
                        vertical-align: bottom;
                        margin-right: 5px;
                    }
                    &:hover{
                        color:#fff;
                        i{
                            color:#14dbcf;
                        }
                    }
                }
            }
            mat-tab-group{
                div.history-box{
                    padding: 30px 50px;
                    background-color: #2b3c4c;
                    h2.title{
                        color:#fff;
                        font-size: 18px;
                        text-align: center;
                        margin-bottom: 30px;
                        font-weight: bold;
                    }
                    div.table-home{
                        table{
                            table-layout: fixed;
                            .mobile-block{
                                display: none;
                            }
                            thead{
                                 background-color: #3e6c96;
                                 tr{
                                     th{
                                         color:#fff;
                                         font-size: 14px;
                                         text-align: center;
                                         font-weight: normal;
                                     }
                                 }
                            }
                            tbody{
                                tr{
                                    background-color: #3c5062;
                                    td{
                                        text-align: center;
                                        color:#94a5b5;
                                        font-size: 14px;
                                        p{
                                            text-overflow: ellipsis;
                                            margin-bottom: 0;
                                            overflow: hidden;
                                            display: inline-block;
                                            cursor: pointer;
                                            width: 80%;
                                            height: 20px;
                                            white-space: nowrap;
                                        }
                                    }
                                    &.no-display{
                                        td{
                                            text-align: center;
                                        }
                                    }
                                    &:nth-child(odd){
                                        background-color: #2b3c4c;
                                    }
                                }
                            }
                        }
                        nav{
                            ul{
                                text-align: center;
                                justify-content: center;
                                margin-bottom: 0;
                                li{
                                    a.page-link{
                                        color:#627281;
                                        background: none;
                                        border:none;
                                        font-size: 12px;
                                        font-weight: bold;
                                        i{
                                            font-size: 10px;
                                        }
                                    }
                                    &.active{
                                        a{
                                            color:#95a6b5;
                                        }
                                    }
                                }
                            }
                        }
                        div.more-btn{
                            text-align: center;
                            button{
                                border-style: none;
                                background: #2d516f;
                                border-radius: 5px;
                                color: #fff;
                                font-size: 13px;
                                padding: 5px 15px;
                            }
                        }
                    }
                }
            }
        }
    }

    // Large devices (desktops, 992px and up)
    @media (max-width: 1024px) {

        div.game-container.graph{
            div.game-wrap{
                div.game-left{
                    width:50%;
                }
                div.game-right{
                    width:50%;
                }
            }
        }

    }

    // Extra large devices (large desktops, 1200px and up)
    @media (min-width: 1025px) and (max-width: 1225px){

        div.game-container.graph{
            div.game-wrap{
                div.game-left{
                    width:43%;
                }
                div.game-right{
                    width:57%;
                }
            }
        }
    }

    // mobile or tablets
    @media (max-width: 992px) {}


    @media (max-width: 768px) {
        div.game-container.graph{
            padding: 15px;
            div.game-wrap{
                flex-wrap: wrap;
                div.game-left{
                    width:100%;
                    padding: 15px;
                    order:1;
                }
                div.game-right{
                    width:100%;
                    padding: 15px;
                }
            }
            div.bonus-game-info{
                padding: 15px;
            }
            div.history-wrap{
                mat-tab-group{
                    div.history-box{
                        padding: 20px 15px;
                        div.table-home{
                            table{
                                .mobile-none{
                                    display:none;
                                }
                                .mobile-block{
                                    display: block;
                                    mat-accordion{
                                        mat-expansion-panel{
                                            border-radius: 0 !important;
                                            background: none;
                                            &:not([class*=mat-elevation-z]){
                                                box-shadow: none;
                                            }
                                            mat-expansion-panel-header{
                                                padding: 0;
                                                &:hover{
                                                    background: none;
                                                }
                                                mat-panel-title{
                                                    color:#94a5b5;
                                                    font-size: 14px;
                                                }
                                                span.mat-expansion-indicator{
                                                    padding-right: 5px;
                                                    &::after{
                                                        color:#94a5b5;
                                                    }
                                                }
                                            }
                                            div.mat-expansion-panel-body{
                                                padding: 0;
                                                div.game-info{
                                                    display: flex;
                                                    margin-bottom:5px;
                                                    color:#94a5b5;
                                                    font-size: 14px;
                                                    span{
                                                        // width: 20%;
                                                        color:#94a5b5;
                                                        text-align: left;
                                                        margin-right: 5px;
                                                    }
                                                    p{
                                                        color:#53e996;
                                                        text-align: left;
                                                    }
                                                    &.data{
                                                        text-align: right;
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }


    // Medium devices (tablets, 768px and up)
    @media (min-width: 577px) and (max-width: 992px) {


    }


    // Small devices (landscape phones, 576px and up)
    @media (max-width: 576px) {

    }

}
