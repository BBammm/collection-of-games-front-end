.mat-tab-header{
    border-color:#34424f;
}
.mat-tab-group.mat-primary .mat-ink-bar {
    background-color:#14dbcf;
}
mat-tab-group{
    .mat-tab-header-pagination-controls-enabled .mat-tab-header-pagination{
        display:none;
    }
    .mat-tab-header-pagination{
        display:none;
    }
    .mat-tab-label{
        opacity: 1;
        color:#8fa8c0;
        &.mat-tab-label-active{
            color: #fff;
        }
    }
}

div.game-container.mine{
    max-width: 1230px;
    padding: 60px 15px;
    div.game-wrap{
        display: flex;
        div.game-left{
            padding: 30px;
            background-color: #15212c;
            width: 35%;
            order: 1;
            div.user-balance{
                display:flex;
                align-items:center;
                justify-content: space-between;
                p.title{
                    display: inline-block;
                    margin-bottom: 0;
                    color:#95a6b5;
                    font-size: 13px;
                    img{
                        margin-right: 5px;
                    }
                    span.point{
                        margin-left: 10px;
                        font-size: 13px;
                        color:#fff;
                        font-weight: bold;
                    }
                }
                button.guide-btn{
                    border:none;
                    background: none;
                    color: #586d7f;
                    font-size: 20px;
                    &:hover{
                        color: #14dbcf;
                    }
                }
            }
            div.game-controll-pannel{
                margin: 30px 0 20px 0;
                ul.game-level{
                    list-style: none;
                    padding: 0;
                    display:flex;
                    margin-bottom: 0;
                    justify-content: space-between;
                    li{
                        flex-basis: 24%;
                        height: 80px;
                        border-radius:5px;
                        box-shadow: inset 0px -4px 0 0 rgba(0, 0, 0, 0.19);
                        background-color: #3b4d5b;
                        background-image: url('/assets/games/mine/images/select-back.png');
                        background-repeat:no-repeat;
                        background-position:-8px 8px;
                        padding: 10px;
                        position: relative;
                        cursor: pointer;
                        p.level{
                            text-align: right;
                            position: absolute;
                            right:10px;
                            bottom:0;
                            margin-bottom: 0;
                            font-size:36px;
                            font-family: 'Russo One', 'Noto Sans KR', sans-serif;
                            color: #89a0b1;
                            text-shadow: 4px 0 0 #3b4d5b, -4px 0 0 #3b4d5b, 0 4px 0 #3b4d5b, 0 -4px 0 #3b4d5b, 4px 4px #3b4d5b, -4px -4px 0 #3b4d5b, 4px -4px 0 #3b4d5b, -4px 4px 0 #3b4d5b;
                        }
                        &.selected{
                            background-image: url('/assets/games/mine/images/select-back-on.png');
                            p.level{
                                color:#53e996;
                            }
                        }
                    }
                }
            }
            div.bet-calculator{
                p.title{
                    font-size: 14px;
                    color:#95a6b5;
                    margin-bottom: 10px;
                }
            }
            div.betting-btn{
                margin-top: 20px;
                margin-bottom: 30px;
                button{
                    background: #14dbcf;
                    border-radius:5px;
                    color:#fff;
                    width: 100%;
                    border-style: none;
                    height: 60px;
                    font-weight: bold;
                    transition: all 0.3s ease;
                    &:hover{
                        background-color:#0fcec2;
                    }
                    &.restart{
                        background:#53e996;
                    }
                }
            }
            div.control{
                display:flex;
                div.label-box{
                    word-break: break-all;
                    width: 30%;
                    display: flex;
                    flex-wrap: wrap;
                    align-items: center;
                    justify-content: center;
                    p.standing-label{
                        font-size:14px;
                        color:#95a6b5;
                        margin-bottom: 0;
                        width: 100%;
                    }
                    p.value{
                        margin-bottom: 0;
                        font-size: 18px;
                        color:#53e996;
                        font-weight: bold;
                        width: 100%;
                    }
                    &.stake{
                        width: 40%;
                        padding-left: 25px;
                        border-left:1px solid #2e3e4c;
                    }
                    button.cashout{
                        border-radius: 5px;
                        border-style: none;
                        height: 35px;
                        background-color: #2d516f;
                        font-size: 12px;
                        color:#9ebbd4;
                        width: 100%;
                    }
                }
            }
            div.code-wrap{
                // border:1px solid pink;
                margin-top: 25px;
                div.code-box{
                    p.title{
                        font-size: 14px;
                        color:#95a6b5;
                        margin-bottom: 5px;
                    }
                    div.field{
                        display:flex;
                        mat-form-field{
                            width: 85%;
                            .mat-form-field-wrapper{
                                .mat-form-field-outline{
                                    border-radius: 5px 0 0 5px;
                                }
                            }
                        }
                        button{
                            width: 15%;
                            height: 35px;
                            border-style: none;
                            background: #223342;
                            border-radius:0 5px 5px 0;
                        }
                    }
                }
            }
        }
        div.game-right{
            padding: 30px;
            background-color: #1a2732;
            width: 65%;
            div.game-round-text{
                position: relative;
                p.title{
                    font-size: 22px;
                    font-weight: bold;
                    margin-bottom: 15px;
                    color:#91a2b2;
                    span{
                        color:#fff;
                    }
                }
            }
            div.mine-container{
                margin:0 auto;
                div.feed{
                    max-width: 470px;
                    max-height: 470px;
                    width: 100%;
                    // height: 100%;
                    margin:0 auto;
                    position: relative;
                    div.explain-div-outer {
                        width: 100%;
                        height: 100%;
                        position: absolute;
                        z-index: 9999;
                        display:flex;
                        flex-wrap: wrap;
                        justify-content: center;
                        padding: 30px 40px 30px 30px;
                        background-image:url('/assets/games/mine/images/textbox-back.png');
                        background-repeat: no-repeat;
                        background-color: #1a2732;
                        color:#95a6b5;
                        font-size: 13px;
                        background-size: 100%;
                        background-position: center;
                    }
                    div.game{
                        width: 100%;
                        height: 100%;
                        position: relative;
                        overflow: visible;
                        opacity: 1;
                        mat-icon{
                            width: 100%;
                            height: 100%;
                            position: relative;
                            img{
                                position: absolute;
                                left: 50%;
                                top: 50%;
                                transform:translate(-50%,-50%);
                                width: 100%;
                            }
                        }
                        &.faded{
                            opacity: 0.6;
                            // margin-bottom: 10px;
                        }
                        ul.board{
                            width: 100%;
                            height: 100%;
                            margin-bottom: 0;
                            padding: 0;
                            list-style: none;
                            display:flex;
                            flex-wrap: wrap;
                            justify-content: space-between;
                            li{
                                width: 19%;
                                height: 90px;
                                box-shadow: inset 0px -4px 0 0 rgba(0, 0, 0, 0.19);
                                background-color: #3b4d5b;
                                border-radius:5px;
                                cursor: pointer;
                                overflow: hidden;
                                padding: 9px;
                                margin-bottom: 5px;
                                &.pressed{
                                    background-color: #21313d;
                                    background-image: url('/assets/games/mine/images/money-icon.png');
                                    background-repeat: no-repeat;
                                    background-position: 15% 70%;
                                }
                                &.bomb{
                                    background-image: none;
                                }
                                span.tile-val{
                                    color:#fff;
                                    font-weight: bold;
                                    font-size: 14px;
                                    display:block;
                                    text-align: right;
                                    display:inline-block;
                                    word-break: break-all;
                                    margin-bottom: 0;
                                    width: 100%;
                                }
                            }
                        }
                    }
                }
            }
        }
    }
    div.history-wrap{
        background-color: #2b3c4c;
        margin-top:30px;
        div.verification-btn{
            text-align: right;
            background-color: #212e3a;
            margin-bottom: 10px;
            padding-bottom: 10px;
            button{
                font-size: 13px;
                border-style: none;
                outline: none;
                background: none;
                color:#95a6b5;
                padding: 6px 15px;
                border-radius: 15px;
                background-color: #273643;
                i{
                    color:#626f7c;
                    font-size: 19px;
                    vertical-align: bottom;
                    margin-right: 5px;
                }
                &:hover{
                    color:#fff;
                    i{
                        color:#14dbcf;
                    }
                }
            }
        }
        mat-tab-group{
            div.history-box{
                padding: 30px 50px;
                background-color: #2b3c4c;
                h2.title{
                    color:#fff;
                    font-size: 18px;
                    text-align: center;
                    margin-bottom: 30px;
                    font-weight: bold;
                }
                div.table-home{
                    table{
                        table-layout: fixed;
                        .mobile-block{
                            display: none;
                        }
                        thead{
                             background-color: #3e6c96;
                             tr{
                                 th{
                                     color:#fff;
                                     font-size: 14px;
                                     text-align: center;
                                     font-weight: normal;
                                 }
                             }
                        }
                        tbody{
                            tr{
                                background-color: #3c5062;
                                td{
                                    text-align: center;
                                    color:#94a5b5;
                                    font-size: 14px;
                                    p{
                                        text-overflow: ellipsis;
                                        margin-bottom: 0;
                                        overflow: hidden;
                                        display: inline-block;
                                        cursor: pointer;
                                        width: 80%;
                                        height: 20px;
                                        white-space: nowrap;
                                    }
                                }
                                &.no-display{
                                    td{
                                        text-align: center;
                                    }
                                }
                                &:nth-child(odd){
                                    background-color: #2b3c4c;
                                }
                            }
                        }
                    }
                    nav{
                        ul{
                            text-align: center;
                            justify-content: center;
                            margin-bottom: 0;
                            li{
                                a.page-link{
                                    color:#627281;
                                    background: none;
                                    border:none;
                                    font-size: 12px;
                                    font-weight: bold;
                                    i{
                                        font-size: 10px;
                                    }
                                }
                                &.active{
                                    a{
                                        color:#95a6b5;
                                    }
                                }
                            }
                        }
                    }
                    div.more-btn{
                        text-align: center;
                        button{
                            border-style: none;
                            background: #2d516f;
                            border-radius: 5px;
                            color: #fff;
                            font-size: 13px;
                            padding: 5px 15px;
                        }
                    }
                }
            }
        }
    }
}


// Large devices (desktops, 992px and up)

@media (min-width: 1024px) {

    div.game-container.mine{
        div.game-wrap{
            div.game-right{
                min-height: 597px;
            }
        }
    }

}
@media (max-width: 1024px) {

    div.game-container.mine{
        div.game-wrap{
            div.game-left{
                width:50%;
            }
            div.game-right{
                width:50%;
                div.mine-container{
                    div.feed{
                        div.game{
                            ul.board{
                                li{
                                    height: 68px;
                                    &.pressed{
                                        background-size: 50%;
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }

}

// Extra large devices (large desktops, 1200px and up)
@media (min-width: 1025px) and (max-width: 1225px){

    div.game-container.mine{
        div.game-wrap{
            div.game-left{
                width:43%;
            }
            div.game-right{
                width:57%;
            }
        }
    }
}

// mobile or tablets
@media (max-width: 992px) {}


@media (max-width: 768px) {
    div.game-container.mine{
        padding: 15px;
        div.game-wrap{
            flex-wrap: wrap;
            div.game-left{
                width:100%;
                padding: 15px;
                order:1;
                div.game-history-wrap{
                    padding: 10px;
                }
            }
            div.game-right{
                width:100%;
                padding: 15px;
                div.mine-container{
                    div.feed{
                        div.game{
                            ul.board{
                                li{
                                    height: 90px;
                                }
                            }
                        }
                    }
                }
            }
        }

        div.game-bets-container{
            padding: 25px 15px;
            ul.columns-list{
                li.column{
                    width: 49%;
                }
            }
        }

        div.history-wrap{
            mat-tab-group{
                div.history-box{
                    padding: 20px 15px;
                    div.table-home{
                        table{
                            .mobile-none{
                                display:none;
                            }
                            .mobile-block{
                                display: block;
                                mat-accordion{
                                    mat-expansion-panel{
                                        border-radius: 0 !important;
                                        background: none;
                                        &:not([class*=mat-elevation-z]){
                                            box-shadow: none;
                                        }
                                        mat-expansion-panel-header{
                                            padding: 0;
                                            &:hover{
                                                background: none;
                                            }
                                            mat-panel-title{
                                                color:#94a5b5;
                                                font-size: 14px;
                                            }
                                            span.mat-expansion-indicator{
                                                padding-right: 5px;
                                                &::after{
                                                    color:#94a5b5;
                                                }
                                            }
                                        }
                                        div.mat-expansion-panel-body{
                                            padding: 0;
                                            div.game-info{
                                                display: flex;
                                                margin-bottom:5px;
                                                color:#94a5b5;
                                                font-size: 14px;
                                                span{
                                                    // width: 20%;
                                                    color:#94a5b5;
                                                    text-align: left;
                                                    margin-right: 5px;
                                                }
                                                p{
                                                    color:#53e996;
                                                    text-align: left;
                                                }
                                                &.data{
                                                    text-align: right;
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}


// Medium devices (tablets, 768px and up)
@media (min-width: 577px) and (max-width: 992px) {


}


// Small devices (landscape phones, 576px and up)
@media (max-width: 576px) {
    div.game-container.mine{
        div.game-wrap{
            div.game-left{
                div.control{
                    div.label-box{
                        width: 25%;
                        &.stake{
                            width: 50%;
                        }
                    }
                }
            }
            div.game-right{
                div.mine-container{
                    div.feed{
                        div.game{
                            ul.board{
                                li{
                                    height: 60px;
                                    &.pressed{
                                        background-size: 40%;
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
